{
	"name": "mcctools.github.io",
	"version": "0.0.1",
	"scripts": {
		"dev-tauri": "cross-env MY_MODE=tauri npm run tauri dev",
		"build-tauri": "cross-env MY_MODE=tauri npm run tauri build",
		"dev": "dev-tauri",
		"build": "build-tauri",
		"dev-ghpages": "cross-env MY_MODE=ghpages npm run svelte-kit dev",
		"build-ghpages": "cross-env MY_MODE=ghpages npm run svelte-kit build",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. .",
		"tauri": "tauri",
		"svelte-kit": "svelte-kit"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^1.0.0-next.34",
		"@sveltejs/kit": "next",
		"@tauri-apps/cli": "^1.0.0-rc.13",
		"@types/cookie": "^0.4.1",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"cross-env": "^7.0.3",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"svelte": "^3.46.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.1",
		"tslib": "^2.3.1",
		"typescript": "~4.6.2"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/fira-mono": "^4.5.0",
		"@tauri-apps/api": "^1.0.0-rc.6",
		"cookie": "^0.4.1"
	}
}
